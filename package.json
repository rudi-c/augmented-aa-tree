{
  "name": "augmented-aa-tree",
  "version": "1.0.0",
  "description": "Immutable balanced binary search tree (AA tree) augmented with order statistics",
  "main": "_build/src/index.js",
  "types": "_build/src/aatree.d.ts",
  "scripts": {
    "build": "tsc -d",
    "bench": "tsc && node --expose-gc _build/benchmark/benchmark.js",
    "profile": "tsc && node --prof _build/benchmark/profile.js",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts' || true",
    "test": "tsc && ava --verbose _build/test"
  },
  "keywords": [
    "immutable",
    "aa",
    "balanced",
    "binary",
    "search",
    "tree",
    "order",
    "augmented",
    "order",
    "statistics"
  ],
  "author": "Rudi Chen",
  "license": "ISC",
  "devDependencies": {
    "@collectable/red-black-tree": "^4.2.0",
    "@types/node": "^8.0.20",
    "@types/underscore": "^1.8.1",
    "ava": "^0.21.0",
    "collections": "^5.0.7",
    "functional-red-black-tree": "^1.0.1",
    "immutable": "^3.8.1",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "underscore": "^1.8.3"
  }
}
